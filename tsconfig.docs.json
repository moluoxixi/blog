{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "noImplicitAny": false,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["./docs/.vitepress/**/*", "./docs/.vitepress/*"],
  "exclude": [".docs/.vitepress/cache/**/*", ".docs/.vitepress/config.*"]
}
